    "cacheTTL": 86400000,
    "syncPriority": {
      "high": ["test_execution", "coverage_analysis"],
      "medium": ["test_generation", "quality_metrics"],
      "low": ["general"]
    }
  },
  "security": {
    "enableAuth": true,
    "allowedPeers": [],
    "requireTLS": false
  },
  "monitoring": {
    "enableMetrics": true,
    "metricsInterval": 5000,
    "healthCheckInterval": 30000
  }
}
